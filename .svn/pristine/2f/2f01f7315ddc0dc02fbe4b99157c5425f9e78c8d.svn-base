<body>
<p align="center" class="style1">Job Application</p>
<form id="form1" name="form1" method="post" action="">

<table border = 0>

<tr>
	<td width = 200><strong>Job Id:</strong></td>
	<td><input type="text" readonly = "readonly" size = 35 name="firstname" id="firstname" />*</td>
</tr>
<tr>
	<td width = 200><strong>Job Title:</strong></td>
	<td><input type="text" readonly = "readonly" size = 35 name="firstname" id="firstname" />*</td>
</tr>

<tr>
	<!-- To be passed on by the previous page  -->
	<!--readonly = "readonly"-->
	<td width = 200><strong>Student Id:  </strong></td>
	<td><input type = "text" size = 35 name="studentid" id="studentid" /><br /> </td>
</tr>

<tr>
	<td width = 200><strong>Name:</strong></td>
	<td><input type="text" readonly = "readonly" size = 35 name="firstname" id="firstname" />*</td>
</tr>

<tr>
	<td width = 200><strong>Address:</strong></td>
	<td><input type="text" size = 35 name="address" id="address" /></td>
</tr>

<tr>
	<td width = 200><strong>Sex:</strong></td>
	<td><select name = "sex" id = "sex"> 
	<option>Select One</option>
  	<option>Male</option>
 	<option>Female</option>
  	<option>Do not want to disclose</option>
	</select> </td>
</tr>

<tr>
	<td width = 200><strong>Email:</strong></td>
	<td><input type="text" size = 35 name="email" id = "email"/>*</td>
</tr>

<tr>
	<td width = 200><strong>Phone Number</strong></td>
	<td><input type="text" size = 35 name="phonenumber" id="phonenumber" /></td>
</tr>

<tr>
	<td width = 200><strong>Visa Status:</strong></td>
	<td><select name = "visa" id = "visa">
  		<option>Select One</option>
  		<option>US Citizen</option>
  		<option>F1</option>
  		<option>J1</option>
  		<option>H1B</option>
  		<option>N/A</option>
		</select></td>
</tr>


<tr>
	<td width = 200><strong>Courses Taken:</strong></td>
	<td><input type="text" size = 35 size = 35 name="coursestaken" id="coursestaken" /></td>
</tr>

<tr>
	<td width = 200><strong>Educational Qualification:</strong></td>
	<td><input type="text" size = 35 size = 35 name="educationalqualification" id="educationalqualification" /></td>
</tr>

<tr>
	<td width = 200><strong>Expected Salary:</strong></td>
	<td><input type="text" size = 35 name="expectedsalary" id="expectedsalary" /></td>
</tr>

<tr>
	<td width = 200><strong>Technical Skills:</strong></td>
	<td><input type="text" size = 35 name="technicalskills" id="technicalskills" /></td>
</tr>

<tr>
	<td width = 200><strong>Experience/Achievements:</strong></td>
	<td><input type="text" size = 35 name="expach" id="expach" /></td>
</tr>

<tr>
	<td width = 200><strong>Industry:</strong></td>
	<td><input type="text" size = 35 name="industry" id="industry" /></td>
</tr>

<tr>
	<td width = 200><strong>Location Preference:</strong></td>
		<td><select name = "location" id = "location">
  		<option>Select One</option>
  		<option>No preference</option>
  		<option>Northern States</option>
  		<option>Southern States</option>
  		<option>Eastern States</option>
  		<option>Western States</option>
		</select></td>
</tr>

<tr>
  <td><strong>Upload Resume:</strong></td>
  <td><input type="file" size = 35 name = "resume" id = "resume" /></td>
</tr>
<tr>
  <td><strong>Start Date:</strong></td>
  <td><select name = "startmonth" id = "startmonth">
